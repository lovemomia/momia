<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="abstractOperateApi" class="cn.momia.common.core.api.HttpServiceApi" abstract="true">
        <property name="service" value="${Service.Operate}" />
    </bean>

    <bean class="cn.momia.api.operate.ConfigServiceApi" parent="abstractOperateApi" />
    <bean class="cn.momia.api.operate.FeedbackServiceApi" parent="abstractOperateApi" />
</beans>